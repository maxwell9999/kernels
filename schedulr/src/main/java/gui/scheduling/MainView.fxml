<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.paint.Color?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="1000.0" prefWidth="1400.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <MenuBar fx:id="menuBar" VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="File">
               <items>
                  <MenuItem fx:id="createMenuItem" mnemonicParsing="false" onAction="#createMenuItemClicked" text="Create Schedule" />
                  <MenuItem fx:id="openMenuItem" mnemonicParsing="false" onAction="#openMenuItemClicked" text="Open Schedule" />
                  <MenuItem fx:id="saveMenuItem" mnemonicParsing="false" onAction="#saveMenuItemClicked" text="Save Schedule" />
                  <MenuItem mnemonicParsing="false" onAction="#publishMenuItemClicked" text="Publish Schedule" />
               </items></Menu>
        <Menu mnemonicParsing="false" text="Window">
          <items>
            <MenuItem fx:id="resourceMenuItem" mnemonicParsing="false" onAction="#resourceMenuItemClicked" text="Resources" />
            <MenuItem fx:id="preferenceMenuItem" mnemonicParsing="false" onAction="#preferenceMenuItemClicked" text="Preferences" />
            <MenuItem fx:id="importMenuItem" mnemonicParsing="false" onAction="#importMenuItemClicked" text="Import" />
            <MenuItem fx:id="feedbackMenuItem" mnemonicParsing="false" onAction="#feedbackMenuItemClicked" text="Feedback" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Settings">
          <items>
            <MenuItem fx:id="logOut" mnemonicParsing="false" onAction="#logoutMenuItemClicked" text="Logout" />
                  <MenuItem fx:id="aboutMenuItem" mnemonicParsing="false" onAction="#aboutMenuItemClicked" text="About Schedulr" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <SplitPane fx:id="splitPane" dividerPositions="0.1005567928730512, 0.9905567928730512" focusTraversable="true" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <items>
        <AnchorPane>
          <children>
            <Label alignment="CENTER" layoutX="14.0" layoutY="14.0" minWidth="60.0" prefWidth="-1.0" style="&#10;" text="Master" textAlignment="CENTER" wrapText="false">
              <font>
                <Font size="18.0" fx:id="x1" />
              </font>
              <textFill>
                <Color blue="0.624" green="0.624" red="0.624" fx:id="x2" />
              </textFill>
            </Label>
          </children>
        </AnchorPane>
        <ScrollPane prefHeight="-1.0" prefWidth="-1.0">
          <content>
          	<VBox fx:id="mainPane">
          	    <children>
          	    	<HBox spacing="2">
          	    		<children>
          	    			<Button fx:id="addPanelButton" text="Add a class" />
          	    			<Button fx:id="editPanelButton" text="Edit a class" />
          	    			<Button fx:id="rmPanelButton" text="Remove a class" />
          	    		</children>
					</HBox>
		            <AnchorPane fx:id="calendarPane" minHeight="-1.0" minWidth="-1.0" prefHeight="545.0" prefWidth="430.0">
		              <children>
		              </children>
	            	</AnchorPane>
	            </children>
            </VBox>
          </content>
        </ScrollPane>
      	<ScrollPane>
          <content>
	        <AnchorPane>
	          	<children>
	          			<VBox fx:id="addPane">
	          				<children>
	            			</children>
	            		</VBox>
	 		  		</children>
	        	</AnchorPane>
        	</content>
     	</ScrollPane>
      </items>
    </SplitPane>
    <HBox id="HBox" alignment="CENTER_LEFT" spacing="5.0" VBox.vgrow="NEVER">
      <children>
        <Label maxHeight="1.7976931348623157E308" maxWidth="-1.0" text="Left status" HBox.hgrow="ALWAYS">
          <font>
            <Font size="11.0" fx:id="x3" />
          </font>
          <textFill>
            <Color blue="0.625" green="0.625" red="0.625" fx:id="x4" />
          </textFill>
        </Label>
        <Pane prefHeight="-1.0" prefWidth="-1.0" HBox.hgrow="ALWAYS" />
        <Label font="$x3" maxWidth="-1.0" text="Right status" textFill="$x4" HBox.hgrow="NEVER" />
      </children>
      <padding>
        <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
      </padding>
    </HBox>
  </children>
</VBox>
